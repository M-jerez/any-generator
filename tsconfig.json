{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": true,
		"moduleResolution":"node"
	},
	"exclude": [
		"node_modules/**",
		"doc/**",
		"dist/**",
		"wordpress/**"
	],
	"compileOnSave": false
}
