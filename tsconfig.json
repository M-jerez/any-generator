{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": false,
		"noImplicitAny": false,
		"removeComments": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"sourceMap": false,
		"moduleResolution":"node",
		"outDir":"dist/"
	},
	"exclude": [
		"node_modules",
		"dist",
		"test"
	],
	"compileOnSave": true
}
