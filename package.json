{
	"name": "anygen",
	"version": "0.4.0",
	"description": "Simple scaffolding code generation for any project.",
	"main": "dist/lib/Builder.js",
	"repository": {
		"type": "git",
		"url": "git+https://M-jerez@github.com/M-jerez/gulp-cli-generator.git"
	},
	"engines": {
		"node": ">=10 <=12"
	},
	"keywords": [
		"generator",
		"cli",
		"scallfolding"
	],
	"author": "Ma Jerez (https://twitter.com/Ma_jrz)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/M-jerez/gulp-cli-generator/issues"
	},
	"homepage": "https://github.com/M-jerez/gulp-cli-generator#readme",
	"devDependencies": {
		"@types/node": "^12.12.35",
		"chai": "^4.2.0",
		"husky": "^4.2.5",
		"prettier": "^2.0.4",
		"pretty-quick": "^2.0.1",
		"rimraf": "^3.0.2",
		"tslint": "^6.1.1",
		"tslint-config-prettier": "^1.18.0",
		"typedoc": "^0.17.4",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"commander": "^5.0.0"
	},
	"bin": {
		"anygen": "dist/bin/anygen.js"
	},
	"scripts": {
		"test": "npm run lint",
		"test_old": "node node_modules/mocha/bin/mocha dist/test/*.spec.js --reporter spec",
		"precompile": "npm run clean",
		"clean": "rimraf dist/*",
		"dev": "tsc -w",
		"format": "prettier --write .",
		"format-s": "pretty-quick --staged",
		"lint": "tslint -c tslint.json 'src/**'",
		"lint-f": "tslint --fix -c tslint.json 'src/**'",
		"anygen": "node ./dist/bin/anygen.js",
		"pc": "pretty-quick --staged & npm run lint"
	},
	
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged & npm run lint"
		}
	}
}
